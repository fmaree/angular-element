    "build:elements:testsonar": "ng test --watch=false --code-coverage && ng build --prod --output-hashing none && node build-elements.js",
    "test": "ng test --watch=false --code-coverage",
    "test:prepush": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
